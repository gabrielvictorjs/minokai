  "tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5b5f71"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
			],
			"settings": {
				"foreground": "#b5b4c9"
			}
		},
		{
			"name": "Function names/calls",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#a0b9d8"
			}
		},
		{
			"name": "Function call arguments",
			"scope": [
				"meta.function-call.arguments",
			],
			"settings": {
				"fontStyle": ""
			},
		},
		{
			"name": "Special/Builtin Variables",
			"scope": [
				"variable.language",
			],
			"settings": {
				"foreground": "#a18daf"
			}
		},
		{
			"name": "Constants and Literals",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"constant.other.color",
				"constant.other.rune",
			],
			"settings": {
				"foreground": "#a18daf"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"punctuation.definition.string",
				"string.regexp",
			],
			"settings": {
